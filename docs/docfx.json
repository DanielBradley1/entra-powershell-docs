      { "dest": "module", "exclude": [ "toc.yml" ], "files": [ "**/*.yml" ], "src": "reference/beta", "version": "entra-powershell-beta" },
      { "dest": "module/entra-powershell-beta", "files": [ "toc.yml" ], "src": "reference/beta", "version": "entra-powershell-beta" },

      { "dest": "entra-powershell", "files": [ "**/*.md", "**/*.yml" ], "src": "virtual-folder" },
      { "dest": "module", "files": [ "**/*.md", "**/*.yml" ], "src": "virtual-folder-module" }
