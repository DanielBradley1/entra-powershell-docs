{
  "build": {
    "content": [
      {
        "files": [ "**/*.md", "**/*.yml" ],
        "src": "docs-conceptual",
        "dest": "docs-conceptual",
        "version": "entra-powershell"
      },
      {
        "files": [ "**/*.yml", "**/About/*.md" ],
        "src": "samples-v2.0",
        "dest": "module",
        "version": "entra-powershell"
      },
      {
        "files": [ "**/*.yml", "**/About/*.md" ],
        "src": "samples-v3.0",
        "dest": "module",
        "version": "entra-powershell-beta"
      },
      {
        "files": [ "toc.yml" ],
        "src": "breadcrumb",
        "dest": "samples/breadcrumb"
      }
    ],
    "versions": {
      "entra-powershell": { "dest": "samples-v2.0" },
      "entra-powershell-beta": { "dest": "samples-v3.0" }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId": "MSDocsHeader-Entra",
      "breadcrumb_path": "/powershell/samples/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "Standard"
    },
    "template": [],
    "markdownEngineName": "markdig",
    "exportRawModel": true,
    "exportViewModel": true
  }
}
